{% extends 'base.html.twig' %}

{% block title %}{{parent()}}{% endblock %}

{% block headerBreadcrumb %}
	<li class="breadcrumb-item">
		<a href='{{ path('index') }}'>
			<i class="feather icon-home"></i>
		</a>
	</li>
	<li class="breadcrumb-item">
		<a>Carga</a>
	</li>
{% endblock headerBreadcrumb %}


{% block body %}
<center>
<div class="col-md-6">
 <div class="card mt-4">  
  <div class="row align-items-center text-center">
   
    <div class="card-body">
         {{form(cargaForm)}}
    </div>
   </div>
  </div>
 </div>
</center>
{% endblock %}

{% block stylesheets %}
    {{ parent() }}
	{{ hello_bootstrap_table_css() }}
{% endblock %}

{% block javascripts %}
    {{ parent() }}
	{{ hello_bootstrap_table_js() }}
{% endblock %}
